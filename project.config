	description = Nexa/HomeEasy control library with absolute dim support.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Calle Gunnarsson (calle-gunnarsson)
[access "refs/for/refs/*"]
	read = group user/Calle Gunnarsson (calle-gunnarsson)
