	submit = group user/Calle Gunnarsson (calle-gunnarsson)
[access "refs/heads/*"]
	read = group user/Calle Gunnarsson (calle-gunnarsson)
	create = group user/Calle Gunnarsson (calle-gunnarsson)
	push = +force group user/Calle Gunnarsson (calle-gunnarsson)
	pushMerge = group user/Calle Gunnarsson (calle-gunnarsson)
[access "refs/tags/*"]
	push = +force group user/Calle Gunnarsson (calle-gunnarsson)
	pushTag = group user/Calle Gunnarsson (calle-gunnarsson)
